{
  "name": "harperdb-node",
  "version": "1.1.0",
  "description": "JavaScript library written in TypeScript for generating HarperDB requests",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run format && npm run build",
    "start": "npm run format && npm run build"
  },
  "keywords": [
    "HarperDB",
    "HDB",
    "Node"
  ],
  "homepage": "https://github.com/Xoelos/harperdb-node",
  "author": "Samuel Krohn",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2.6.6"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.10",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  }
}